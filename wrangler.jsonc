{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "pathology-annotation",
  "compatibility_date": "2025-11-25",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "pathology-db-production",
      "database_id": "local-dev-db"
    }
  ],
  "r2_buckets": [
    {
      "binding": "IMAGES",
      "bucket_name": "pathology-images"
    }
  ]
}